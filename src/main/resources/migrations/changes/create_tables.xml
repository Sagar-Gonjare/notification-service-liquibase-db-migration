<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">

	<changeSet id="200420242-1" author="Sagar Gonjare">

		<createTable tableName="notification" schemaName="notification_service_liquibase_db">
			<column name="notification_code" type="int"></column>

			<column name="client_id" type="varchar(250)"></column>

			<column name="mode" type="varchar(250)"></column>

			<column name="subject" type="varchar(250)"></column>

			<column name="body" type="varchar(250)"></column>

			<column name="footer" type="varchar(250)"></column>

			<column name="from_email" type="varchar(250)"></column>

			<column name="to_email" type="varchar(250)"></column>

			<column name="updated_date" type="varchar(250)"></column>

			<column name="created_date" type="varchar(250)"></column>

		</createTable>

	</changeSet>


</databaseChangeLog>
